<%= form_for @manage_court_form, url: resource do |f| %>
	<fieldset>
		<legend><%= header %></legend>
		<ul>
			<li>
				<%= f.label :number, "Court number" %>
				<%= f.text_field :number %>
			</li>
			<%= render "court_times", f: f, court_times: :opening_times, objects: @manage_court_form.opening_times %>
			<%= render "court_times", f: f, court_times: :peak_times, objects: @manage_court_form.peak_times %>
			<%= f.submit "Submit court" %>
		</ul>
	</fieldset>
<% end %>