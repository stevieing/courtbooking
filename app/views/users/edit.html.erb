<section id="mydetails" class="jazzyform">
  <h2><%= header %></h2>
  <%= render "error_messages", target: @user %>
  <%= form_for @user do |f| %>
    <fieldset>
      <%= render "user", f: f %>
      <ul class="submit"><li><%= f.submit "Submit user" %></li></ul>
    </fieldset>
  <% end %>
</section>